/*动画八：四分之一圆加载动画  */
.loader-circle{
  width: 100px;
  height: 100px;
  display: inline-block;
  position: relative;
}
.loader-circle:after{
  content: '';
  display: block;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  border: 2px solid #ddd;
  border-top:2px solid yellowgreen;
  position: absolute;
  top: calc(50% - 25px);
  left:calc(50% - 25px);
  animation: loader-circle 1s infinite ease;
}
@keyframes loader-circle{
  0%{
    transform: rotate(0deg);
  }
  100%{
    transform: rotate(360deg);
  }
}
/*动画九：四分之三圆加载动画  */
.loading-circle{
  width: 100px;
  height: 100px;
  display: inline-block;
  position: relative;
}
.loading-circle:after{
  content: '';
  display: block;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  border: 2px solid brown;
  border-top:2px solid transparent;
  position: absolute;
  top: calc(50% - 25px);
  left:calc(50% - 25px);
  animation: loader-circle 1s infinite ease;
}
@keyframes loader-circle{
  0%{
    transform: rotate(0deg);
  }
  100%{
    transform: rotate(360deg);
  }
}
/*动画十：地球围绕太阳转  */
.loading-sun{
  width: 50px;
  height: 50px;
  margin: 25px;
  border-radius:50%; 
  display: inline-block;
  position: relative;
  border: 1px solid #000;
  animation: loader-circle 2s infinite linear;
}
.loading-sun::before{
  content: '';
  display: block;
  width: 20px;
  height: 20px;
  border-radius: 50%;
  background-color: blue;
  position: absolute;
  top: calc(50% - 10px);
  left: calc(50% - 10px);
}
.loading-sun::after{
  content: '';
  display: block;
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background-color: orange;
  position: absolute;
  top:  -5px;
  left: calc(50% - 5px);
  box-shadow: 0 0 10px orange;
}
/*动画十一：心跳  */
.loading-heart{
  width: 100px;
  height: 100px;
  display: inline-block;
  position: relative;
  animation: loading-heart 1s ease infinite;
}
.loading-heart::before{
  content: '';
  display: block;
  width: 20px;
  height: 40px;
  background-color: red;
  border-radius: 10px 10px 0 0;
  position: absolute;
  top: 40%;
  left: calc(50% - 7px) ;
  transform-origin: center bottom;
  transform:translateY(-50%) rotate(45deg);
}
.loading-heart::after{
  content: '';
  display: block;
  width: 20px;
  height: 40px;
  background-color: red;
  border-radius: 10px 10px 0 0;
  position: absolute;
  top:40%;
  left: calc(50% + 7px) ;
  transform-origin: center bottom;
  transform:translateY(-50%) rotate(-45deg);
}
@keyframes loading-heart{
  0%{
    transform: scale(.4);
  }
  50%{
    transform: scale(1);
  }
  100%{
    transform: scale(.4);
  }
}
/*动画十二：环形跑道  */
.loading-spinner{
  width: 32px;
  height: 16px;
  margin: 42px;
  animation: loading-spinner 2s steps(60) infinite;
}
@keyframes loading-spinner{
  0%{
    background-position: center 0;
  }
  100%{
    background-position: center -960px;
  }
}